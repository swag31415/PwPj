<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<Accordion fx:id="bankPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onMouseEntered="#attent" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: burlywood;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PwPj.App">
  <panes>
    <TitledPane animated="false" text="Add Entry">
         <content>
            <AnchorPane style="-fx-background-color: burlywood;">
               <children>
                  <TextField fx:id="titleBox" layoutX="30.0" layoutY="30.0" onAction="#addEntryTitle" prefHeight="40.0" prefWidth="250.0" promptText="Entry Title" style="-fx-background-color: white;" />
                  <TextField fx:id="entryBox" alignment="TOP_LEFT" layoutX="30.0" layoutY="100.0" onAction="#addEntry" prefHeight="220.0" prefWidth="540.0" promptText="Entry" style="-fx-background-color: White;" />
               </children>
            </AnchorPane>
         </content></TitledPane>
  </panes>
</Accordion>
